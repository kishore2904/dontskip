    sswRun(function() {
      var intervalToLoad = setInterval(function() {
        if(ssw('#ssw-fave-side-backdrop').length){
          ssw('<div id="ssw-fave-remove"></div>').appendTo(ssw('#ssw-fave-side-backdrop'))
          clearInterval(intervalToLoad)
        }
        ssw('.ssw-fave-caption').on('click', function() {
          ssw('.ssw-fave-product-price').each(function (index, element) {
            if(ssw(element)[0].childNodes[0].nodeValue === ssw(element)[0].childNodes[1].innerHTML) {
              ssw(element)[0].childNodes[1].remove()
            }
          })
        });
        clearInterval(intervalToLoad)
      }, 1000)
      }); 
  </script>
