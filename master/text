<script>
  $( document ).ready(function() {
    $('#product-price').each(function(){
      var pricelist = $(this).html();
      //var x=123456524578;
      //alert(x);
      x = pricelist.replace(/,/g,'').replace(/\s+/g,',').replace(/^,/g,'').replace(/,$/g,'');
      x=x.toString();
      var lastThree = x.substring(x.length-3);
      var otherNumbers = x.substring(0,x.length-3);
      if(otherNumbers != '')
          lastThree = ',' + lastThree;
      var res = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + lastThree;
      finalamount = res.replace( /Rs.,/ ,'Rs. ');
       $(this).html(finalamount)
      //alert(res);
    });
  });
  
</script>
