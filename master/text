<script>
  // Features accordeon
   $('.dl-dynamic dt').on('click', function() {
     $(this).toggleClass('active');
   }) 
   $('.emi-popup-btn').on('click',function(){
   	$('.sticky-header-prepared .header-shop').css('z-index','1');
    $('#bundle-smart_bar').css('z-index','0');
   });
   $('.emi-close-btn').on('click',function(){
	$('.sticky-header-prepared .header-shop').css('z-index','350');
     $('#bundle-smart_bar').css('z-index','1999999999');
   })
   $('.w3-modal').on('click',function(){
//    	$(this).css('display','none');
   });
  $(document).mouseup(function (e) {
         var emipopup = $(".w3-modal-content");
         if (!$('.emi-popup-btn').is(e.target) && !emipopup.is(e.target) && emipopup.has(e.target).length == 0) {
            $('.w3-modal').css('display','none');
           	$('.sticky-header-prepared .header-shop').css('z-index','350');
     		$('#bundle-smart_bar').css('z-index','1999999999');
         }
    });
   
  
//   Price Formate JS
  $( document ).ready(function() {
  $('.price-list').each(function(){
    var pricelist = $(this).html();
    //var x=123456524578;
    //alert(x);
    x = pricelist.replace(/,/g,'').replace(/\s+/g,',').replace(/^,/g,'').replace(/,$/g,'');
    x=x.toString();
    var lastThree = x.substring(x.length-3);
    var otherNumbers = x.substring(0,x.length-3);
    
    if(otherNumbers != '')
        lastThree = ',' + lastThree;
    var res = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + lastThree;
    finalamount = res.replace( /Rs.,/ ,'Rs. ').replace( /Rs. ,/,'Rs. ' );
     $(this).html(finalamount)
    //alert(res);
  });
    
  });
  $( document ).ready(function() {
    $('#product-price').each(function(){
      var pricelist = $(this).html();
      //var x=123456524578;
      //alert(x);
      x = pricelist.replace(/,/g,'').replace(/\s+/g,',').replace(/^,/g,'').replace(/,$/g,'');
      x=x.toString();
      var lastThree = x.substring(x.length-3);
      var otherNumbers = x.substring(0,x.length-3);
      if(otherNumbers != '')
          lastThree = ',' + lastThree;
      var res = otherNumbers.replace(/\B(?=(\d{2})+(?!\d))/g, ",") + lastThree;
      finalamount = res.replace( /Rs.,/ ,'Rs. ').replace( /Rs. ,/,'Rs. ' );
       $(this).html(finalamount)
      //alert(res);
    });
  });
  
</script>
  
